  "scripts": {
    "build": "webpack --config ./src/production.babel.js --mode development --env ./webpack/config.babel.js",
    "build:dev": "webpack --config ./src/production.babel.js --mode development --env ./webpack/config.babel.js",
    "build:prod": "webpack --config ./src/production.babel.js --mode production --env ./webpack/config.babel.js",
    "ce:dev": "node ./bin/ce-webpack.js --config src --init webpack/config.babel.js",
    "ce:prod": "node ./bin/ce-webpack.js --config src --init webpack/config.babel.js",
    "ce:start": "node ./bin/ce-webpack.js --config src --init webpack/config.babel.js --serve",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "start": "webpack serve --progress --config src/development.babel.js --mode development --env webpack/config.babel.js",
    "warnings": "webpack --performance-hints warning"
